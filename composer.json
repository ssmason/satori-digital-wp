{
    "name": "ssmason/satori-digital-build",
    "type": "project",
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/ssmason/satori-digital.git"
      },
      {
        "type": "git",
        "url": "https://github.com/ssmason/guttenberg-tech-carousel.git"
      }
    ],
    "authors": [
      {
        "name": "Steve Mason",
        "homepage": "https://github.com/ssmason/"
      }
    ],
    "require": { 
      "composer/installers": "~1.0", 
      "ssmason/tech-carousel": "~1.0.0",
      "ssmason/satori-digital": "~1.0.0"
    },
    "require-dev": { 
    },
    "scripts": {
    },
    "config": {
      "platform": {
        "php": "8.2"
      },
      "allow-plugins": {
        "composer/installers": true,
        "dealerdirect/phpcodesniffer-composer-installer": true
      }
    },
    "extra": {
      "installer-paths": {
        "wordpress/wp-content/plugins/tech-carousel": [
            "ssmason/tech-carousel"
          ] ,
        "wordpress/wp-content/themes/satoridigital": [
            "ssmason/satori-digital"
          ] 
      }
    }
  }
  